      <default>100.0</default>
    </key>
    <key type="d" name="height">
      <default>100.0</default>
    </key>
    <key type="i" name="aspect-ratio-width">
      <default>1</default>
    </key>
    <key type="i" name="aspect-ratio-height">
      <default>1</default>
    </key>
    <key name="aspect-ratio" enum="org.x.pix.GthAspectRatio">
      <default>'image'</default>
    </key>
    <key type="b" name="aspect-ratio-invert">
      <default>true</default>
    </key>
    <key type="b" name="high-quality">
      <default>true</default>
    </key>
  </schema>

</schemalist>