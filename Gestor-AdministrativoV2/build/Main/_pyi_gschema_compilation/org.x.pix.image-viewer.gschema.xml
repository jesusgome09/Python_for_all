<?xml version="1.0" encoding="UTF-8"?>
<schemalist>

  <schema path="/org/x/pix/image-viewer/" id="org.x.pix.image-viewer">
    <key name="zoom-quality" enum="org.x.pix.GthZoomQuality">
      <default>'high'</default>
    </key>
    <key name="zoom-change" enum="org.x.pix.GthZoomChange">
      <default>'fit-size-if-larger'</default>
    </key>
    <key name="transparency-type" enum="org.x.pix.GthTranspType">
      <default>'none'</default>
      <description>Possible values are: white, black, checked, none.</description>
    </key>
    <key type="b" name="reset-scrollbars">
      <default>true</default>
      <description>Whether to reset the scrollbars position after changing image</description>
    </key>
    <key name="check-type" enum="org.x.pix.GthCheckType">
      <default>'midtone'</default>
    </key>
    <key name="check-size" enum="org.x.pix.GthCheckSize">
      <default>'medium'</default>
    </key>
  </schema>

</schemalist>